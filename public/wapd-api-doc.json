                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "status": {
                      "type": "string"
                    },
                    "CategoryId": {
                      "type": "integer"
                    },
                    "category": {
                      "type": "string"
                    },
                    "images": {
                      "type": "array",
                      "example": "[{image Object}, {image Object}]"
                    },
                    "size": {
                      "type": "array",
                      "example": "[S, M, L]"
                    },
                    "color": {
                      "type": "array",
                      "example": "[blue, red, green]"
                    },
                    "stock": {
                      "type": "array",
                      "example": "[1, 3, 4]"
                    },
                    "origin_price": {
                      "type": "integer"
                    },
                    "sell_price": {
                      "type": "integer"
                    },
                    "isFavorited": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
